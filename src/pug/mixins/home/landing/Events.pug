mixin Event(config)
  .col-md-6.mb-4
    .card.h-100.shadow.px-2.px-lg-3.card-span.pt-4
      .text-center.text-md-start.card-hover
        .card-body
          .d-flex.align-items-center
            span.badge.bg-soft-primary.text-primary.fs--1.fw-light.p-3.rounded-1!=config.date
            h6.fw-light.fs-1.fs-lg-2.text-start.ms-3=config.title
          p.mt-4.mb-md-0.mb-lg-3.fw-light.lh-base.text-start=config.text
          .d-flex.flex-between-center
            .d-flex.align-items-center
              a.btn.btn-lg.ps-0.text-primary.fw-light.fs--1(href='#' role='button') Learn more
                svg.bi.bi-arrow-right-short(xmlns='http://www.w3.org/2000/svg' width='25' height='25' fill='currentColor' viewBox='0 0 16 16')
                  path(fill-rule='evenodd' d='M4 8a.5.5 0 0 1 .5-.5h5.793L8.146 5.354a.5.5 0 1 1 .708-.708l3 3a.5.5 0 0 1 0 .708l-3 3a.5.5 0 0 1-.708-.708L10.293 8.5H4.5A.5.5 0 0 1 4 8z')

            .d-flex.align-items-center
              svg.bi.bi-alarm.me-2(xmlns='http://www.w3.org/2000/svg' width='16' height='16' fill='currentColor' viewBox='0 0 16 16')
                path(d='M8.5 5.5a.5.5 0 0 0-1 0v3.362l-1.429 2.38a.5.5 0 1 0 .858.515l1.5-2.5A.5.5 0 0 0 8.5 9V5.5z')
                path(d='M6.5 0a.5.5 0 0 0 0 1H7v1.07a7.001 7.001 0 0 0-3.273 12.474l-.602.602a.5.5 0 0 0 .707.708l.746-.746A6.97 6.97 0 0 0 8 16a6.97 6.97 0 0 0 3.422-.892l.746.746a.5.5 0 0 0 .707-.708l-.601-.602A7.001 7.001 0 0 0 9 2.07V1h.5a.5.5 0 0 0 0-1h-3zm1.038 3.018a6.093 6.093 0 0 1 .924 0 6 6 0 1 1-.924 0zM0 3.5c0 .753.333 1.429.86 1.887A8.035 8.035 0 0 1 4.387 1.86 2.5 2.5 0 0 0 0 3.5zM13.5 1c-.753 0-1.429.333-1.887.86a8.035 8.035 0 0 1 3.527 3.527A2.5 2.5 0 0 0 13.5 1z')
              p.mb-0.fw-light.text-dark.fs--1=config.time         

mixin Events
  +Section#events
    .row
      .col-lg-7.mx-auto.text-center.mb-4
        h5.fw-light.fs-3.fs-lg-5.lh-sm.mb-3 Upcoming University Events
        p.mb-3 Executive Trade International is an University Application Centre, guiding Bangladeshi students to Australian, British, Canadian and Irish universities!
    .row.flex-center.h-100
      .col-xl-9
        #carouselEvents.carousel.slide(data-bs-ride='carousel')
          .carousel-inner
            .carousel-item.active(data-bs-interval='10000')
              .row.h-100.justify-content-center                
                +Event(events[0])                  
                +Event(events[1])                  
                                
            .carousel-item(data-bs-interval='5000')
              .row.h-100.justify-content-center                
                +Event(events[2]) 
                +Event(events[3]) 
                
            .carousel-item(data-bs-interval='3000')
              .row.h-100.justify-content-center                
                +Event(events[4])   
                +Event(events[5])   
            .carousel-item
              .row.h-100.justify-content-center          
                +Event(events[6])
                +Event(events[0]) 
              
                                           
          .row.mt-3.flex-center
            .col-auto.position-relative.z-index-2
              ol.carousel-indicators
                li.active.mx-2(data-bs-target='#carouselEvents' data-bs-slide-to='0')
                li.mx-2(data-bs-target='#carouselEvents' data-bs-slide-to='1')
                li.mx-2(data-bs-target='#carouselEvents' data-bs-slide-to='2')
                li.mx-2(data-bs-target='#carouselEvents' data-bs-slide-to='3')
